{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon",
    "test": "jest --forceExit",
    "build": "tsc",
    "start:prod": "node dist/server.js"
  },
  "dependencies": {
    "@google-cloud/error-reporting": "^0.2.2",
    "@google-cloud/logging-winston": "^0.7.0",
    "@nestjs/common": "^4.4.0",
    "@nestjs/core": "^4.4.0",
    "@nestjs/microservices": "^4.4.0",
    "@nestjs/swagger": "^1.1.2",
    "@nestjs/testing": "^4.4.0",
    "@nestjs/websockets": "^4.4.0",
    "@types/chance": "^0.7.35",
    "@types/cron": "^1.2.1",
    "@types/request": "^2.0.9",
    "@types/request-promise-native": "^1.0.10",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.7",
    "chance": "^1.0.12",
    "circuit-breaker-js": "^0.0.1",
    "consul": "^0.30.0",
    "cron": "^1.3.0",
    "mongoose": "^4.13.5",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.10",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "rxjs": "^5.4.0",
    "scraperjs": "^1.2.0",
    "typescript": "^2.5.2",
    "uuid": "^3.1.0",
    "winston": "^2.4.0",
    "winston-loggly-bulk": "^2.0.2"
  },
  "devDependencies": {
    "@types/consul": "^0.23.32",
    "@types/jest": "^21.1.8",
    "@types/mongoose": "^4.7.28",
    "@types/node": "^8.0.28",
    "@types/supertest": "^2.0.4",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.3",
    "ts-node": "^3.3.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
